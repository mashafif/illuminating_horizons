    "import overpass\n",
    "import geojson\n",
    "states = [\"New Hampshire\",\"New York\",\"New Mexico\",\"North Carolina\",\"North Dakota\",\"Rhode Island\",\"South Carolina\",\"South Dakota\",\"West Virginia\"]\n",
    "api = overpass.API(timeout=600)\n",
    "road_type =[\"primary\",\"secondary\",\"tertiary\",\"motorway\",\"trunk\",\"road\"]\n",
    "root_path = os.path.abspath('../')\n",
    "raw_data_path = os.path.join(root_path,\"raw_data\")\n",
    "\n",
    "for state in states:\n",
    "    for road in road_type:\n",
    "        try:\n",
    "            query = f\"\"\"\n",
    "            area[\"name\"=\"{state}\"]->.a;\n",
    "            way(area.a)[\"highway\"~\"^({road})$\"];\n",
    "            out geom;\n",
    "            \"\"\"    \n",
    "            result = api.get(query)\n",
    "    \n",
    "            file_name= os.path.join(raw_data_path,f\"USA_roads/{state}_{road}_location.geojson\")\n",
    "    \n",
    "            with open(file_name,mode=\"w\") as f:\n",
    "                geojson.dump(result,f)\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"Error in downloading {road} data for {state}: {e}\")\n",
    "        else:\n",
    "            print(f\"Successfully downloaded {road} data for {state}\")        "
   ]
  },
